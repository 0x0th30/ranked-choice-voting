  },
  "dependencies": {
    "@prisma/client": "^5.1.0",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "prisma": "^5.1.0",
    "redis": "^4.6.7",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "scripts": {
    "sca": "yarn audit",
    "lint": "eslint src tests",
    "lint:fix": "eslint --fix src tests",
    "test": "jest --verbose",
    "test:silent": "jest --silent --verbose",
    "start:prisma": "prisma generate && prisma migrate deploy",
    "start:dev": "yarn start:prisma && ts-node-dev -r tsconfig-paths/register src/main.ts",
    "start:prod": "yarn start:prisma && node dist/main.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored"
  },
  "resolutions": {
    "json5": "^1.0.2"
  }
